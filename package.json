{
    "name": "@snowytime/xanax",
    "version": "0.0.0",
    "description": "",
    "type": "module",
    "scripts": {
        "test": "mocha --exit",
        "build:raw": "rm -rf dist && swc src --config-file .swcrc-mjs.json -d dist/mjs && swc src --config-file .swcrc-cjs.json -d dist/cjs && sh finalize.sh",
        "build:esm": "rm -rf dist && swc src --config-file .swcrc-mjs.json -d dist",
        "build:types:esm": "tsc -p ./tsconfig.mjs.json",
        "build:types": "tsc -p ./tsconfig.mjs.json && tsc -p ./tsconfig.cjs.json",
        "build": "npm run build:raw && npm run build:types && sh finalize.sh",
        "release:patch": "npm run test && npm run build && git add . && git commit -m '⚡️ auto patch' && git push origin master && npm version patch && npm publish"
    },
    "keywords": [],
    "author": "snowytime",
    "license": "MIT",
    "homepage": "https://snowy.sh/packages/xanax",
    "bugs": {
        "url": "https://github.com/snowytime/xanax/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/snowytime/xanax.git"
    },
    "devDependencies": {
        "@snowytime/standards": "^0.0.8",
        "@swc/cli": "^0.1.59",
        "@types/chai": "^4.3.4",
        "@types/mocha": "^10.0.1",
        "@types/node": "^18.11.18",
        "@types/sinon": "^10.0.13",
        "chai": "^4.3.7",
        "chai-as-promised": "^7.1.1",
        "dotenv": "^16.0.3",
        "mocha": "^10.2.0",
        "sinon": "^15.0.1",
        "ts-node": "^10.9.1",
        "typescript": "^4.9.4"
    },
    "main": "dist/cjs/index.js",
    "module": "dist/mjs/index.js",
    "exports": {
        ".": {
            "import": "./dist/mjs/index.js",
            "require": "./dist/cjs/index.js"
        }
    },
    "engines": {
        "node": ">=16.0.0"
    },
    "files": [
        "dist"
    ]
}
